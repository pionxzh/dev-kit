{
  "name": "@textea/dev-kit",
  "version": "0.12.0",
  "description": "Toolkit for developing Augmented Textea Products.",
  "license": "MIT",
  "main": "./dist/umd/index.cjs",
  "module": "./dist/es/index.mjs",
  "types": "./dist/es/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/es/index.d.mts",
      "import": "./dist/es/index.mjs"
    },
    "./sheet": {
      "types": "./dist/es/sheet/index.d.mts",
      "import": "./dist/es/sheet/index.mjs"
    },
    "./theme": {
      "types": "./dist/es/theme/index.d.mts",
      "import": "./dist/es/theme/index.mjs"
    },
    "./python": {
      "types": "./dist/es/python/index.d.mts",
      "import": "./dist/es/python/index.mjs"
    },
    "./utils": {
      "types": "./dist/es/utils/index.d.mts",
      "import": "./dist/es/utils/index.mjs"
    },
    "./config/*": "./config/*"
  },
  "repository": "https://github.com/TexteaInc/dev-kit",
  "bugs": {
    "url": "https://github.com/TexteaInc/dev-kit/issues"
  },
  "homepage": "https://github.com/TexteaInc/dev-kit#readme",
  "files": [
    "dist",
    "config"
  ],
  "scripts": {
    "build": "env-cmd -e production run-s clean build:tsc build:rollup",
    "build:rollup": "rollup -c -m",
    "build:tsc": "tsc -p tsconfig.json",
    "test": "vitest",
    "clean": "rimraf ./dist",
    "start": "env-cmd -e development run-p start:tsc start:rollup",
    "start:rollup": "rollup -c -w",
    "start:tsc": "tsc --watch -p tsconfig.json",
    "prepare": "husky install",
    "lint:check": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "changelog": "git-cliff --output CHANGELOG.md",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "@emotion/react": "*",
    "@emotion/styled": "*",
    "@mui/material": "^5",
    "uuid": "^8"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-angular": "^17.1.0",
    "@discordjs/ts-docgen": "^0.4.1",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/material": "^5.10.3",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/node": "^18.7.14",
    "@types/react": "^18.0.17",
    "@types/uuid": "^8.3.4",
    "@types/web": "^0.0.72",
    "@typescript-eslint/eslint-plugin": "^5.36.0",
    "@typescript-eslint/parser": "^5.36.0",
    "env-cmd": "^10.1.0",
    "eslint": "=8.22.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-react": "^7.31.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "expect-type": "^0.13.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.78.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-typescript2": "^0.33.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.11",
    "typescript": "^4.7.4",
    "uuid": "^8.3.2",
    "vitest": "^0.22.1"
  },
  "packageManager": "yarn@3.2.2"
}
