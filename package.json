{
  "name": "@textea/dev-kit",
  "version": "0.12.7",
  "description": "Toolkit for developing Augmented Textea Products.",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/es/index.d.mts",
  "author": "himself65 <himself65@textea.co>",
  "exports": {
    "./dist/": "./dist/",
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/es/index.d.mts",
      "import": "./dist/es/index.mjs"
    },
    "./sheet": {
      "types": "./dist/es/sheet/index.d.mts",
      "import": "./dist/es/sheet/index.mjs"
    },
    "./theme": {
      "types": "./dist/es/theme/index.d.mts",
      "import": "./dist/es/theme/index.mjs"
    },
    "./python": {
      "types": "./dist/es/python/index.d.mts",
      "import": "./dist/es/python/index.mjs"
    },
    "./utils": {
      "types": "./dist/es/utils/index.d.mts",
      "import": "./dist/es/utils/index.mjs"
    },
    "./google-cloud": {
      "types": "./dist/es/google-cloud/index.d.mts",
      "import": "./dist/es/google-cloud/index.mjs"
    },
    "./config/*": "./config/*"
  },
  "repository": "https://github.com/TexteaInc/dev-kit",
  "bugs": {
    "url": "https://github.com/TexteaInc/dev-kit/issues"
  },
  "homepage": "https://github.com/TexteaInc/dev-kit#readme",
  "files": [
    "dist",
    "config",
    "LICENSE"
  ],
  "scripts": {
    "build:tsc": "tsc -b",
    "build:rollup": "rollup -c rollup.config.ts",
    "build": "yarn run build:tsc && yarn run build:rollup --configPlugin swc3",
    "test": "vitest",
    "prepare": "husky install",
    "lint:check": "eslint . --ext .js,.ts,.mts,.tsx",
    "lint:fix": "eslint . --ext .js,.ts,.mts,.tsx --fix",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@google-cloud/cloudbuild": "^3.1.0",
    "@google-cloud/run": "^0.2.2",
    "@typescript-eslint/eslint-plugin": "^5.36.0",
    "@typescript-eslint/parser": "^5.36.0",
    "@vitejs/plugin-react": "^2.1.0",
    "eslint": "^8.23.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-react": "^7.31.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unused-imports": "^2.0.0"
  },
  "peerDependencies": {
    "@emotion/react": "*",
    "@emotion/styled": "*",
    "@mui/material": "^5",
    "uuid": "*"
  },
  "peerDependenciesMeta": {
    "@emotion/react": {
      "optional": true
    },
    "@emotion/styled": {
      "optional": true
    },
    "@mui/material": {
      "optional": true
    },
    "uuid": {
      "optional": true
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-angular": "^17.1.0",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/material": "^5.10.4",
    "@rollup/plugin-sucrase": "^4.0.4",
    "@swc/core": "^1.2.249",
    "@types/node": "^18.7.14",
    "@types/react": "^18.0.17",
    "@types/uuid": "^8.3.4",
    "@types/web": "^0.0.72",
    "expect-type": "^0.14.2",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^2.79.0",
    "rollup-plugin-swc3": "^0.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2",
    "uuid": "^9.0.0",
    "vitest": "^0.23.1"
  },
  "packageManager": "yarn@3.2.2"
}
